<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>EggButterflyFertilized</defName>
    <label>butterfly egg (fert.)</label>
    <description>Fertilized butterfly egg. If all goes well, it should hatch into a baby butterfly. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(236,217,184)</color>
      <drawSize>1.1</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>11.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>5</hatcherDaystoHatch>
        <hatcherPawn>PoisonButterfly</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  <ThingDef ParentName="EggUnfertBase">
    <defName>EggButterflyUnFertilized</defName>
    <label>butterfly egg (unfert.)</label>
    <description>Unfertilized butterfly egg. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(236,217,184)</color>
      <drawSize>1.1</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>11.0</MarketValue>
    </statBases>
  </ThingDef>
  <ThingDef ParentName="OrganicProductBase">
    <defName>Silk</defName>
    <label>silk</label>
    <description>Silk from an animal.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Synthread</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MarketValue>4</MarketValue>
      <Mass>0.025</Mass>
      <MaxHitPoints>150</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>3</DeteriorationRate>
    </statBases>
    <stuffProps>
      <categories>
        <li>Fabric</li>
      </categories>
      <commonality>0.15</commonality>
      <color>(174,219,228)</color>
      <statOffsets>
        <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
        <ArmorRating_Sharp>0.03</ArmorRating_Sharp>
        <ArmorRating_Heat>0.03</ArmorRating_Heat>
      </statOffsets>
      <statFactors>
        <ArmorRating_Blunt>1.0</ArmorRating_Blunt>
        <ArmorRating_Sharp>1.65</ArmorRating_Sharp>
        <ArmorRating_Heat>4.0</ArmorRating_Heat>
        <Insulation_Cold>1.25</Insulation_Cold>
        <Insulation_Heat>1.25</Insulation_Heat>
      </statFactors>
    </stuffProps>
    <thingCategories>
      <li>Textiles</li>
    </thingCategories>
  </ThingDef>
</Defs>
